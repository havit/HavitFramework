<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net48</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<!-- NuGet -->
	<Import Project="../NuGet.targets" />
	<PropertyGroup>
		<PackageVersion>2.0.36</PackageVersion>
		<Description>HAVIT .NET Framework Extensions - Web Class Library for HAVIT Business Layer</Description>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Havit.Business\Havit.Business.csproj" />
		<ProjectReference Include="..\Havit.Web\Havit.Web.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="System.Web" Pack="false" />
	</ItemGroup>

</Project>
