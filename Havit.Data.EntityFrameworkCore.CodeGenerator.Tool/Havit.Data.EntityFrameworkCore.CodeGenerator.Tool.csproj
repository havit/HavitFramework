<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
	</PropertyGroup>

	<!-- NuGet -->
	<Import Project="../NuGet.targets" />
	<PropertyGroup>
		<PackageVersion>2.9.1</PackageVersion> <!--Při aktualizaci pozor na situaci, že verze 2.9.2-pre1 už na nuget.org existuje, ale byla unlistována.-->
		<PackAsTool>true</PackAsTool>
		<ToolCommandName>efcodegenerator</ToolCommandName>
		<Description>HAVIT .NET Framework Extensions - Entity Framework Core Extensions - Code Generator (dotnet tool)</Description>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="9.0.0" />
	  <PackageReference Include="Microsoft.VisualStudio.Threading.Analyzers" Version="17.14.15">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	</ItemGroup>
</Project>
